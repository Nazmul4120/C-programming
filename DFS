#include <stdio.h>
#include<conio.h>
void dfs(int);
int g[10][10],visited[10],n,vertex[10];
void main() {
 int i,j;
 clrscr();
 printf("\n\nPlease enter number of vertices:");
 scanf("%d",&n);
 printf("Please enter the values of vertices:");
 for(i=0;i<n;i++)
 scanf("%d",&vertex[i]);
 printf("\nPlease enter adjecency matrix of the graph:\n");
 for(i=0;i<n;i++)
 for(j=0;j<n;j++)
scanf("%d",&g[i][j]);
 for(i=0;i<n;i++)
 visited[i]=0;
 dfs(0);
 getch();
}
void dfs(int i) {
 int j;
 printf("%d ",vertex[i]);
 visited[i]=1;
 for(j=0;j<n;j++)
 if(!visited[j]&&g[i][j]==1)
 dfs(j);
}
